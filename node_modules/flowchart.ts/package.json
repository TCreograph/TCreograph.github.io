{
  "name": "flowchart.ts",
  "version": "0.1.2",
  "author": "Mister-Hope",
  "description": "Flowchart support",
  "main": "./dist/flowchart.js",
  "types": "./typings/flowchart.d.ts",
  "exports": {
    ".": {
      "types": "./typings/flowchart.d.ts",
      "import": "./dist/flowchart.mjs",
      "require": "./dist/flowchart.js",
      "default": "./dist/flowchart.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mister-Hope/flowchart.js.git"
  },
  "keywords": [
    "flowchart"
  ],
  "homepage": "http://flowchart.js.org/",
  "license": "MIT",
  "files": [
    "dist",
    "typings"
  ],
  "dependencies": {
    "@types/raphael": "2.3.3",
    "raphael": "2.3.0",
    "tslib": "2.4.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "24.0.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "esbuild": "0.17.3",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.8.3",
    "rollup": "3.10.0",
    "rollup-plugin-esbuild": "5.0.0",
    "typescript": "4.9.4",
    "vite": "4.0.4"
  },
  "packageManager": "pnpm@7.25.1",
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "build:dev": "webpack",
    "build:js": "cross-env NODE_ENV=production webpack --config webpack.config.old.js",
    "build:js:dev": "webpack --config webpack.config.old.js",
    "dev": "vite dev",
    "watch": "webpack -w",
    "watch:js": "cross-env NODE_ENV=production webpack --config webpack.config.old.js -w",
    "tsc": "tsc",
    "ts:watch": "tsc --watch",
    "lint": "prettier --check --write . && eslint . --ext .ts,.js --fix",
    "lint:check": "prettier --check . && eslint . --ext .ts,.js"
  }
}